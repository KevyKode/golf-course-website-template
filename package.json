{
  "name": "your-golf-course-template",
  "version": "1.0.0",
  "description": "Professional golf course website template with booking system",
  "main": "server.js",
  "scripts": {
  "start": "node server.js",
  "dev": "nodemon server.js",
  "build": "mkdir -p public/assets/css public/assets/js public/assets/images && cp index.html public/index.html && cp -r assets/images/* public/assets/images/ && cp assets/css/* public/assets/css/ && cp assets/js/* public/assets/js/",
  "test": "jest",
  "test:watch": "jest --watch",
  "migrate": "node scripts/migrate.js",
  "seed": "node scripts/seed.js",
  "deploy": "railway deploy"
},
  "keywords": [
    "golf",
    "course",
    "booking",
    "template",
    "tee-times",
    "membership"
  ],
  "author": "Your Name",
  "license": "MIT",
