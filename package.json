{
  "name": "your-golf-course-template",
  "version": "1.0.0",
  "description": "Professional golf course website template with booking system",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "rm -rf dist && mkdir -p dist/assets/css dist/assets/js dist/assets/images && cp index.html dist/index.html && cp -r assets/images dist/assets/images && npm run build:css && npm run build:js",
    "build:css": "postcss assets/css/main.css -o dist/assets/css/main.min.css",
    "build:js": "terser assets/js/main.js -o dist/assets/js/main.min.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "migrate": "node scripts/migrate.js",
    "seed": "node scripts/seed.js",
    "deploy": "railway deploy"
  },
  "keywords": [
    "golf",
    "course",
    "booking",
    "template",
    "tee-times",
    "membership"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-facebook": "^3.0.0",
    "passport-jwt": "^4.0.1",
    "express-session": "^1.17.3",
    "connect-redis": "^7.1.0",
    "redis": "^4.6.10",
    "@supabase/supabase-js": "^2.38.4",
    "pg": "^8.11.3",
    "stripe": "^14.7.0",
    "@sendgrid/mail": "^8.1.0",
    "twilio": "^4.19.0",
    "axios": "^1.6.2",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "node-cron": "^3.0.3",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.32.6",
    "cloudinary": "^1.41.0",
    "dotenv": "^16.3.1",
    "winston": "^3.11.0",
    "express-winston": "^4.2.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "postcss": "^8.4.32",
    "postcss-cli": "^11.0.0",
    "autoprefixer": "^10.4.16",
    "cssnano": "^6.0.1",
    "terser": "^5.24.0",
    "@types/node": "^20.9.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/your-golf-course-template.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/your-golf-course-template/issues"
  },
  "homepage": "https://yourgolfcourse.com"
}